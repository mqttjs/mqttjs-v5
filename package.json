{
  "//": "TODO: author, contributors",
  "name": "mqtt",
  "version": "5.0.0",
  "description": "A MQTT client library",
  "keywords": [
    "mqtt",
    "publish",
    "subscribe",
    "publish-subscribe",
    "publish/subscribe",
    "client",
    "broker",
    "server"
  ],
  "homepage": "https://github.com/mqttjs/MQTT.js#readme",
  "bugs": "https://github.com/mqttjs/MQTT.js/issues",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=14.13.1"
  },
  "files": [
    "dist"
  ],
  "types": "dist",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && nyc ava | pino-pretty"
  },
  "dependencies": {
    "duplexify": "^4.1.2",
    "end-of-stream": "^1.4.4",
    "mqtt-packet": "^7.1.1",
    "number-allocator": "^1.0.9",
    "pino": "^7.6.3",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@types/duplexify": "^3.6.1",
    "@types/end-of-stream": "^1.4.1",
    "@types/node": "^14.18.10",
    "@types/ws": "^8.2.2",
    "aedes": "^0.46.2",
    "ava": "^4.0.1",
    "nyc": "^15.1.0",
    "pino-pretty": "^7.5.0",
    "typescript": "^4.6.0-beta"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/util"
    ],
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "environmentVariables": {
      "DEBUG": "mqtt-packet:writeToStream"
    },
    "verbose": true,
    "nodeArguments": [
      "--trace-deprecation",
      "--napi-modules"
    ]
  }
}
